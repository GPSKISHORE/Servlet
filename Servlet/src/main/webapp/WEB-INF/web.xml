<web-app>

	<welcome-file-list>
		<welcome-file>Links.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>country</servlet-name>	
		<servlet-class>com.tcs.gpsk.jsServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>country</servlet-name>	
		<url-pattern>/city</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>link</servlet-name>	
		<servlet-class>com.tcs.gpsk.LinkUrl</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>link</servlet-name>	
		<url-pattern>/link</url-pattern>
	</servlet-mapping>
</web-app>